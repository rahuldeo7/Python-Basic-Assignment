     "text": [
      "THis is math\n",
      "5 -1\n"
     ]
    }
   ],
   "source": [
    "class base:\n",
    "    def disp(self):\n",
    "        print('THis is math')\n",
    "        \n",
    "    def add(self,a,b):\n",
    "        return a+b\n",
    "    \n",
    "class derived(base):\n",
    "    def add(self, a, b):\n",
    "        return a-b\n",
    "\n",
    "obj = base()\n",
    "obj1 = derived()\n",
    "obj.disp()\n",
    "print(obj.add(2,3),obj1.add(2,3))"
   ]
  },
